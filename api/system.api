	@doc "获取角色菜单树"
	@handler MenuRoleMenuTreeselect
	get /system/menu/roleMenuTreeselect/:roleId (MenuRoleMenuTreeselectReq) returns (MenuTreeselectResp)

	// ========== ROLE角色管理接口 ==========
	@doc "获取角色信息列表"
	@handler RoleList
	get /system/role/list (RoleListReq) returns (RoleListResp)

	@doc "导出角色信息列表"
	@handler RoleExport
	post /system/role/export (RoleListReq) returns (BaseResp)

	@doc "根据角色编号获取详细信息"
