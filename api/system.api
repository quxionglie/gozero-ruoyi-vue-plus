	get /system/post/list (PostListReq) returns (TableDataInfoResp)

	@doc "导出岗位列表"
	@handler PostExport
	post /system/post/export returns (BaseResp)

	@doc "查询岗位详细"
	@handler PostGetInfo
	get /system/post/:postId (PostGetInfoReq) returns (PostResp)

	@doc "新增岗位"
	@handler PostAdd
	post /system/post (PostReq) returns (BaseResp)

	@doc "修改岗位"
	@handler PostEdit
	put /system/post (PostReq) returns (BaseResp)

	@doc "删除岗位"
