	put /system/user/resetPwd (UserResetPwdReq) returns (BaseResp)

	@doc "状态修改"
	@handler UserChangeStatus
	put /system/user/changeStatus (UserChangeStatusReq) returns (BaseResp)

	@doc "根据用户编号获取授权角色"
